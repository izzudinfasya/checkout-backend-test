{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/events",
      "dest": "/api/events.js"
    },
    {
      "src": "/api/discounts",
      "dest": "/api/discounts.js"
    },
    {
      "src": "/api/reg-types",
      "dest": "/api/reg-types.js"
    },
    {
      "src": "/api/registrants",
      "dest": "/api/registrants.js"
    },
    {
      "src": "/api/process-payment",
      "dest": "/api/process-payment.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
